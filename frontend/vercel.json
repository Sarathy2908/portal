{
  "installCommand": "cd .. && git clone https://github.com/flutter/flutter.git -b stable --depth 1 && cd frontend && ../flutter/bin/flutter doctor && ../flutter/bin/flutter pub get",
  "buildCommand": "../flutter/bin/flutter build web --dart-define=API_BASE_URL=$BACKEND_URL",
  "outputDirectory": "build/web",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        },
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        }
      ]
    }
  ]
}
